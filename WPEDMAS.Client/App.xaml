<Application x:Class="WPEDMAS.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPEDMAS.Client">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/Constants.xaml"/>
                <ResourceDictionary Source="Themes/generic.xaml"></ResourceDictionary>
                <ResourceDictionary>
                    <local:WPEDMASBootstrapper x:Key="bootstrapper"></local:WPEDMASBootstrapper>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
